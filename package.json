    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "peerDependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@types/google-protobuf": "^3.15.6",
    "google-protobuf": "^3.20.1"
